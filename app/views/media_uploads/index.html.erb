<p id="notice"><%= notice %></p>

<h1>Media Uploads</h1>

<table>
  <thead>
    <tr>
      <th>Onesimus test</th>
      <th>Image name</th>
      <th>Location</th>
      <th>Active status</th>
      <th>Del status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @media_uploads.each do |media_upload| %>
      <tr>
        <td><%= media_upload.onesimus_test_id %></td>
        <td><%= media_upload.image_name %></td>
        <td><%= media_upload.location %></td>
        <td><%= media_upload.active_status %></td>
        <td><%= media_upload.del_status %></td>
        <td><%= link_to 'Show', media_upload %></td>
        <td><%= link_to 'Edit', edit_media_upload_path(media_upload) %></td>
        <td><%= link_to 'Destroy', media_upload, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Media Upload', new_media_upload_path %>
